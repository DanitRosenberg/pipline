pipeline {
    agent any
    stages {
        stage ('git'){
            steps {
                    checkout([$class: 'GitSCM', branches: [[name: '*/master']], userRemoteConfigs: [[url: 'https://github.com/DanitRosenberg/first.git']]])
            }
        }
    }
    post {
        failure{
            echo "**error**"
        }

    }
}